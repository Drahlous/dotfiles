set nocompatible
set number
set relativenumber
set autoindent
set showmatch
set ignorecase
set mouse=v
set hlsearch
set incsearch
set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4
set wildmode=longest,list

filetype plugin indent on
syntax on
set mouse=a

set clipboard=unnamedplus 		" System Clipboard

set ttyfast


call plug#begin("~/.vim/plugged")
call plug#end()

